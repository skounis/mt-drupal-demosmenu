{% if inject_in_the_body %}

  <div class="mt_demosmenu">
    <ul class="mt_demosmenu__menu">
      {% if links|length > 0 %}
        {% for link in links %}
          <li class="mt_demosmenu__menu-item mt_demosmenu__menu-item--free"><a href="{{ link.1 }}" target="_blank">{{ link.0 }}</a></li>
          <li>{{ user.username|e }}</li>
        {% endfor %}
      {% else %}
        <li class="mt_demosmenu__menu-item mt_demosmenu__menu-item--premium"><a href="https://morethanthemes.com/drupal-themes/premium" target="_blank">Premium themes</a></li>
        <li class="mt_demosmenu__menu-item mt_demosmenu__menu-item--free"><a href="https://morethanthemes.com/drupal-themes/free" target="_blank">Free themes</a></li>
      {% endif %}
    </ul>
  </div>


  <link rel="stylesheet" type="text/css" href="https://gist.githubusercontent.com/skounis/7fbd9a9b1d0b03a1a099e6c3ab667083/raw/81668d870770b4ebb8e60e325920dcbfd7b71723/live.css">

{% endif %}
